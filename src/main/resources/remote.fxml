<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?scenebuilder-stylesheet ./stylesheet.css?>

<TitledPane fx:id="root" animated="false" prefHeight="296.0" prefWidth="120.0" styleClass="main-title-box" text="untitled" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="uk.ac.bristol.controllers.RemoteController">
   <content>
      <VBox fx:id="container" styleClass="flex-use-title">
         <children>
            <HBox fx:id="buttons" styleClass="titledPaneContents">
               <children>
                  <Button maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onMouseClicked="#fetch" styleClass="flex-use-item-title" text="Fetch" HBox.hgrow="ALWAYS" />
                  <Button maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onMouseClicked="#prune" styleClass="flex-use-item-title" text="Prune" HBox.hgrow="ALWAYS" />
               </children>
            </HBox>
         </children>
      </VBox>
   </content>
</TitledPane>
