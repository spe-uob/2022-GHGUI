<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.VBox?>
<?scenebuilder-stylesheet ./stylesheet.css?>

<TitledPane fx:id="root" collapsible="false" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" styleClass="main-title-box" text="Repo" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="uk.ac.bristol.controllers.InformationController">
   <content>
      <VBox styleClass="flex-use-title">
         <children>
            <TitledPane animated="false" styleClass="main-title-box" text="local">
               <content>
                  <VBox fx:id="local" styleClass="titledPaneContents" />
               </content>
            </TitledPane>
            <TitledPane animated="false" styleClass="main-title-box" text="remote">
               <content>
                  <VBox fx:id="remote" styleClass="titledPaneContents" />
               </content>
            </TitledPane>
         </children>
      </VBox>
   </content>
</TitledPane>
